class Solution {
public:
    int strStr(string haystack, string needle) {
        int n = haystack.size();
        int m = needle.size();
        for(int j = 0; j < n - m; j++) {
            int i = 0;
            while(i < m && haystack[i + j] == needle[i]){
                i++;
            }
            if(i == m) {
                return j;   
            }
        }
        return -1;
    }
};