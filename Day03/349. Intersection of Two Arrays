class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> s1(nums1.begin() , nums1.end());
        unordered_set<int> resultset;
        for(int num : nums2) {
            if(s1.count(num)) {
                resultset.insert(num);
            }
        }
        return vector<int>(resultset.begin() , resultset.end());
    }
};